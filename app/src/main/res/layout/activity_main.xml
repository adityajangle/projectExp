<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:background="#394386"
        android:layout_height="match_parent"
        tools:context=".MainActivity">

    <FrameLayout
            android:layout_width="209dp"
            android:layout_height="48dp"
            android:id="@+id/frameLayout"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="80dp" app:layout_constraintTop_toTopOf="parent">

        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <ImageView
                    android:layout_width="45dp"
                    android:layout_height="43dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:src="@drawable/ic_add_box_black_24dp"
                    android:id="@+id/imgiconAdd" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"
                    android:layout_marginStart="16dp" app:layout_constraintVertical_bias="0.4"/>
            <Button
                    android:text="Add"
                    android:background="#5FFF2B"
                    android:layout_width="108dp"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
                    android:id="@+id/btnAdd" app:layout_constraintStart_toEndOf="@+id/imgiconAdd"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintVertical_bias="0.0"/>
        </android.support.constraint.ConstraintLayout>
    </FrameLayout>
    <FrameLayout
            android:layout_width="209dp"
            android:layout_height="49dp"
            android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:id="@+id/frameLayout2" app:layout_constraintHorizontal_bias="0.509">

        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <ImageView
                    android:layout_width="44dp"
                    android:layout_height="41dp"
                    android:src="@drawable/ic_edit_black_24dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:layout_marginStart="16dp" app:layout_constraintBottom_toBottomOf="parent"
                    android:id="@+id/imgiconEdit" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
            <Button
                    android:text="Edit"
                    android:layout_width="110dp"
                    android:background="#FF8015"
                    android:layout_height="wrap_content" app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent" android:layout_marginEnd="16dp"
                    app:layout_constraintEnd_toEndOf="parent" android:id="@+id/btnedit"
                    app:layout_constraintStart_toEndOf="@+id/imgiconEdit" android:layout_marginStart="8dp"
                    app:layout_constraintHorizontal_bias="1.0" app:layout_constraintVertical_bias="0.0"/>
        </android.support.constraint.ConstraintLayout>
    </FrameLayout>
    <FrameLayout
            android:layout_width="207dp"
            android:layout_height="50dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginTop="20dp"
            app:layout_constraintTop_toBottomOf="@+id/frameLayout2" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.004"
            app:layout_constraintHorizontal_bias="0.514">

        <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <ImageView
                    android:layout_width="41dp"
                    android:layout_height="43dp"
                    app:layout_constraintTop_toTopOf="parent" app:layout_constraintStart_toStartOf="parent"
                    android:src="@drawable/ic_dashboard_black_24dp"
                    android:layout_marginStart="16dp" app:layout_constraintBottom_toBottomOf="parent"
                    android:id="@+id/imgiconDashboard" android:layout_marginTop="5dp" android:layout_marginBottom="5dp"/>
            <Button
                    android:text="Dash Board"
                    android:layout_width="108dp"
                    android:layout_height="wrap_content"
                    android:background="#3285B6"
                    android:layout_marginEnd="16dp" app:layout_constraintEnd_toEndOf="parent"
                    android:id="@+id/btndashboard" app:layout_constraintStart_toEndOf="@+id/imgiconDashboard"
                    android:layout_marginStart="8dp"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintBottom_toBottomOf="parent"/>
        </android.support.constraint.ConstraintLayout>
    </FrameLayout>
</android.support.constraint.ConstraintLayout>